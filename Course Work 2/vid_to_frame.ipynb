   "source": [
    "import cv2\n",
    "video_cap = cv2.VideoCapture('13.mp4')\n",
    "success, img = video_cap.read()\n",
    "count = 0\n",
    "while success:\n",
    "    if(count%9 == 0):\n",
    "        c = count/9\n",
    "        cv2.imwrite(\"13_frame%d.jpg\" %c, img)\n",
    "    success, img = video_cap.read()\n",
    "    count += 1"
   ]
